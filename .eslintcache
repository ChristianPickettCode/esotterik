    console.log(\"connected.\");\n                ws.send(JSON.stringify({\n                    message: \"connect\",\n                    action: \"message\"\n                }));\n            };\n\n            ws.onclose = () => {\n                console.log(\"disconnected.\");\n            };\n\n            ws.onmessage = (msg) =>  {\n                if (msg.type === \"message\") {\n                    const data = JSON.parse(msg.data);\n                    if (data.status === \"connect\") {\n                        setId(data.id);\n                        ws.id = data.id;\n                    }\n                    if (data.status === \"send\") {\n                        parse(data.data, ws);\n                    }\n                }\n            };\n\n            // return () => {\n            //     ws.close();\n            // }\n        }\n\n    }, []);\n\n    return (\n        <>\n            { id ? \n                <div style={{paddingLeft :\"10px\", paddingRight:\"10px\", paddingTop:\"10px\", paddingBottom:\"5px\", backgroundColor:\"white\", borderRadius:\"5px\", margin:\"0\"}}>\n                    <QRCode fgColor=\"#282c34\" value={`${url}/welcome/${id}/?${requestString}&appName=${request.appName}&appID=${request.appID}`}  onClick={() => console.log(id)}/>\n                </div> : \"\"}\n        </>\n    )\n}\n\nexport default Index\n","/Users/christianpickett/Documents/esotterik/src/components/bridge.js",["45"],"import React, { useEffect } from 'react';\nimport UserContextProvider from  \"./UserContextProvider\"\n\nconst Bridge = (props) => {\n    return (\n        <UserContextProvider {...props} />\n    )\n}\n\nexport default Bridge\n",["46","47"],"/Users/christianpickett/Documents/esotterik/src/components/hi.js",[],"/Users/christianpickett/Documents/esotterik/src/components/UserReducer.js",[],"/Users/christianpickett/Documents/esotterik/src/components/UserContextProvider.js",["48"],"import React, { useReducer, useState } from 'react';\nimport Index